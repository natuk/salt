PREFIX mo: <http://purl.org/ontology/mo/>
PREFIX frbr: <http://purl.org/vocab/frbr/core#>
PREFIX dct: <http://purl.org/dc/terms/>
PREFIX slobr: <http://127.0.0.1:8890/slobr/>
PREFIX slickmem: <http://127.0.0.1:8890/slickmem/>
PREFIX ems: <http://127.0.0.1:8890/EMS>
INSERT {
    GRAPH <http://127.0.0.1:8890/EMS> { 
        ?uri a ems:Composer ; 
             rdfs:label ?label ;
        <http://127.0.0.1:8890/salt/in_salt_set> <http://127.0.0.1:8890/saltsets/ems_composers> ;
             <http://127.0.0.1:3000/linkedbrainz_uri> ?linkedbrainz_uri .
    }
}
WHERE { 
    GRAPH <http://127.0.0.1:8890/EMS> {
        ?uri <http://127.0.0.1:3000/contributorRole> "Composer" ;
           rdfs:label ?label ;
           mo:musicbrainz_guid ?mbz_uri .
        BIND(IRI(CONCAT(STR(?mbz_uri), "#_")) as ?linkedbrainz_uri) .
    }
}
