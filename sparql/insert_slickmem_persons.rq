PREFIX mo: <http://purl.org/ontology/mo/>
PREFIX frbr: <http://purl.org/vocab/frbr/core#>
PREFIX dct: <http://purl.org/dc/terms/>
PREFIX slobr: <http://127.0.0.1:8890/slobr/>
PREFIX slickmem: <http://127.0.0.1:8890/slickmem/>
PREFIX ems: <http://127.0.0.1:8890/EMS/>

INSERT { 
    graph <http://127.0.0.1:8890/slickmem> { 
        ?uri a slickmem:BookMaker;
        <http://127.0.0.1:8890/salt/in_salt_set> <http://127.0.0.1:8890/saltsets/slickmem_persons> .
    }
}
WHERE { 
    GRAPH <http://127.0.0.1:8890/slickmem> {
        ?uri  a  foaf:Person ;
            rdfs:label ?label .
        ?outside_uri <http://purl.org/ontology/similarity/Subject> ?uri ;
                     <http://purl.org/ontology/similarity/distance> ?distance .  
        FILTER(?distance = "None0None") . #excludes slickmem automated searches 
    }
}

