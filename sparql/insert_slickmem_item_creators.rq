PREFIX mo: <http://purl.org/ontology/mo/>
PREFIX frbr: <http://purl.org/vocab/frbr/core#>
PREFIX dct: <http://purl.org/dc/terms/>
PREFIX slobr: <http://eeboo.oerc.ox.ac.uk/slobr/>
PREFIX slickmem: <http://eeboo.oerc.ox.ac.uk/slickmem/>
INSERT {
    GRAPH <http://eeboo.oerc.ox.ac.uk/slickmem_item_creators> { 
        ?workuri rdfs:subClassOf slobr:Work ; 
                 frbr:creator ?creatoruri .
        ?creatoruri rdfs:label ?creatorname ;
                    a slickmem:Composer ;
                    <http://eeboo.oerc.ox.ac.uk/salt/in_salt_set> <http://eeboo.oerc.ox.ac.uk/saltsets/slickmem_item_creators> .
    }
}
WHERE { 
    GRAPH  <http://eeboo.oerc.ox.ac.uk/slickmem> {
     ?workuri a mo:MusicalWork ;
              dct:title ?worktitle;
              frbr:creator ?creatorname .
              BIND(IRI(CONCAT(STR(?workuri), "_creator")) as ?creatoruri) .
    }
}

