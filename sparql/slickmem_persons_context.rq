PREFIX mo: <http://purl.org/ontology/mo/>
PREFIX frbr: <http://purl.org/vocab/frbr/core#>
PREFIX dct: <http://purl.org/dc/terms/>
PREFIX sim: <http://purl.org/ontology/similarity/>
SELECT distinct ?uri ?outside_uri ?books
WHERE {{
    {0} 
    ?uri <http://127.0.0.1:8890/salt/in_salt_set> <http://127.0.0.1:8890/saltsets/slickmem_persons> .
    OPTIONAL {{
    ?slickmem_alignment sim:Subject ?uri ;
        sim:Object ?outside_uri ;
        sim:distance ?distance .
    FILTER(?distance = "None0None") .
    }} .
    OPTIONAL {{
        ?b foaf:maker ?uri ;
           rdfs:label ?books .
    }}
}}
